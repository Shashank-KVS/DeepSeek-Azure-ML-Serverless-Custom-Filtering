# Ignore Jupyter Notebook checkpoints
.ipynb_checkpoints/

# Ignore virtual environment
venv/
env/

# Ignore compiled Python files
__pycache__/
*.pyc
*.pyo

# Ignore log and temporary files
*.log
*.tmp

# Ignore system files
.DS_Store
Thumbs.db

# Ignore API keys or sensitive configuration files
config.json
.env
secrets.json

# Ignore dependency management cache
pip-log.txt
pip-delete-this-directory.txt

# Ignore notebook output cache
*.out